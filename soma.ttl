      crm:P82a_begin_of_the_begin "-1025-01-01"^^xsd:date ;
      crm:P82b_end_of_the_end "-950-12-31"^^xsd:date 
  ]] ;
  crm:P39i_was_measured_by [ a crm:E16_Measurement ;
    crm:P40_observed_dimension [ a crm:E54_Dimension ;
      crm:P2_has_type somavoc:Length_outside ;

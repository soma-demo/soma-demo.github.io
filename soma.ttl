      crm:P90_has_value 50 ; 
      crm:P91_has_unit somavoc:Centimeter 
    ] ;
    crm:P40_observed_dimension [ a crm:E54_Dimension ;
      crm:P2_has_type somavoc:Length_inside ;
      crm:P90_has_value 108 ; 
      crm:P91_has_unit somavoc:Centimeter  
    ] ;

@prefix dct:  <http://purl.org/dc/terms/> .
@prefix schema: <http://schema.org/> .

#################################################################
# Cemetery & Cemetery Excavation
#################################################################

ex:cemetery01 a crm:E27_Site ;
  rdfs:label "Cemetery 01"@en ;
  crm:P48_has_preferred_identifier [ a crm:E42_Identifier ;
    crm:P190_has_symbolic_content "cem-01"@en
  ] ;
  crm:P1_is_identified_by [ a crm:E41_Appellation ;
    crm:P190_has_symbolic_content "Name of cemetery"@en
  ] ;
  crm:P156_occupies ex:kalyvia_thorikou ;
  crm:P3_has_note "Cemetery description (some text here)."@en ;
  #crm:P43_has_dimension [ a crm:E54_Dimension ;
    #rdfs:label "Cemetery extent"@en ;
    #crm:P90_has_value "0.38"^^xsd:decimal ;
    #crm:P91_has_unit "hectares"@en
  #] ;
  crm:P43_has_dimension [ a crm:E54_Dimension ;
    crm:P2_has_type "Number of tombs" ;
    crm:P90_has_value "100" 
  ] ;
  crm:P92i_was_brought_into_existence_by [ a crm:E63_Beginning_of_Existence ;
  crm:P4_has_time-span [ a crm:E52_Time-Span ;
    crm:P82a_begin_of_the_begin "-700-01-01"^^xsd:date ;
    crm:P82b_end_of_the_end "-650-12-31"^^xsd:date 
  ]] ;
  crm:P70i_is_documented_in ex:doc-cem-1 ;
  crm:P46_is_composed_of ex:tomb01 .


ex:kalyvia_thorikou a crm:E53_Place ;
  rdfs:label "Kalyvia Thorikou, East Attica, Greece"@en ;
  crm:P168_place_is_defined_by "POINT(23.942 37.815)" .

ex:doc-cem-1 a crm:E31_Document;
  crm:P48_has_preferred_identifier [ a crm:E42_Identifier ;
    crm:P190_has_symbolic_content "Title"@en
  ] ;
  crm:P2_has_type [ a crm:E42_Identifier ;
    crm:P190_has_symbolic_content "Type"@en
  ] ;
  crm:P94_was_created_by [ a crm:E65_Creation ;
    crm:P14_carried_out_by [ a crm:E21_Person];
    crm:P4_has_time-span [ crm:P4_has_time-span [ a crm:E52_Time-Span ;
    crm:P82a_begin_of_the_begin "-700-01-01"^^xsd:date ;
    crm:P82b_end_of_the_end "-650-12-31"^^xsd:date 
    ]]
  ] .


# Cemetery Excavation event
ex:excavation-cemetery01 a crmarchaeo:A9_Archeological_Excavation ;
  rdfs:label "Cemetery 01 Excavation"@en ;
  crm:P7_took_place_at ex:excavation-area;
  crm:P4_has_time-span [a crm:E52_Time-Span ;
    crm:P82a_begin_of_the_begin "2000-01-01"^^xsd:date ;
    crm:P82b_end_of_the_end "2000-12-31"^^xsd:date 
  ] ;
  crm:P14_carried_out_by ex:inst-hellenic-antiquities ;
  crm:P8_took_place_on_or_within ex:cemetery01 ;
  crm:P134i_was_continued_by ex:excavation-tomb01.

ex:excavation-area a crm:E53_Place;
  rdfs:label "Excavation area" ;
  crm:P89_falls_within ex:city-name .

ex:city-name a crm:E53_Place;
  rdfs:label "Modern city name" .

ex:inst-hellenic-antiquities a crm:E74_Group ;
  rdfs:label "Excavation group name"@en .


#################################################################
# Tomb
#################################################################

# Tomb
ex:tomb01 a crm:E18_Physical_Thing ;
  rdfs:label "Tomb 01"@en ;
  crm:P48_has_preferred_identifier [ a crm:E42_Identifier ;
    crm:P190_has_symbolic_content "LC-T01"@en
  ] ;
  crm:P1_is_identified_by [ a crm:E41_Appellation ;
    crm:P190_has_symbolic_content "Tomb LC-T01"@en
  ] ;
  crm:P46_is_composed_of ex:burial01 ;
  crm:P156_occupies ex:kalyvia_thorikou ;
  crm:P3_has_note "Tomb description."@en ;
  crm:P2_has_type "Enclosure type"@en ;
  crm:P92i_was_brought_into_existence_by [ a crm:E63_Beginning_of_Existence ;
    crm:P4_has_time-span [ a crm:E52_Time-Span ;
      crm:P82a_begin_of_the_begin "-700-01-01"^^xsd:date ;
      crm:P82b_end_of_the_end "-650-12-31"^^xsd:date 
  ]] ;
  crm:P39i_was_measured_by [ a crm:E16_Measurement ;
    crm:P40_observed_dimension [ a crm:E54_Dimension ;
      crm:P2_has_type "Lenght"@en ;
      crm:P90_has_value "100" ; 
      crm:P91_has_unit "cm" 
    ] 
  ] .

# Tomb Excavation event
ex:excavation-tomb01 a crmarchaeo:A9_Archeological_Excavation ;
  rdfs:label "Tomb 01 Excavation"@en ;
  crm:P7_took_place_at ex:excavation-area-tomb;
  crm:P4_has_time-span [a crm:E52_Time-Span ;
    crm:P82a_begin_of_the_begin "2000-01-01"^^xsd:date ;
    crm:P82b_end_of_the_end "2000-12-31"^^xsd:date 
  ] ;
  crm:P14_carried_out_by ex:inst-hellenic-antiquities ;
  crm:P8_took_place_on_or_within ex:tomb01 ;
  crm:P134i_was_continued_by ex:excavation-burial01.

ex:excavation-area-tomb a crm:E53_Place;
  rdfs:label "Excavation area tomb" .


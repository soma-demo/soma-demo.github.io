# Object T70-2
soma:objT70-2 a crm:E22_Human-Made_Object ;
  rdfs:label "Object T70-2"@en ;
  crm:P48_has_preferred_identifier [ a crm:E42_Identifier ;
    crm:P190_has_symbolic_content "T70-2"@en 
  ] ;
  crm:P1_is_identified_by [ a crm:E42_Identifier ;
    rdfs:label "Inventory number" ;
    crm:P190_has_symbolic_content "P 23559"@en 
  ] ;
  crm:P55_has_current_location soma:agora_museum_athens ;
  crm:P3_has_note "A lekythos that was found inside the partial flling of T70. It was produced with the three-phase firing technique and is decorated with concentric two sets of semicircles at shoulder seperated by a crosshatched panel framed by three vertical lines. The rest of the body is decorated with blacked out areas or black bands. The lekythos has a conical foot, rounded nearly sphearical body, comparativly narrow and rathher short neck, a sharply everted, slightly misformed rim and rounded lip. The juncture of the shoulder and the neck is marked by a shallow, narrow groove. The vertical handle is attached from the shoulder to the neck below the rim."@en ; 
  crm:P44_has_condition [ a crm:E3_Condition_State ;
    rdfs:label "Preservation" ;
    crm:P3_has_note "Almost completly reconstructed from many fragments except for a greater part of the rim and smaller parts of the body and base. The missing parts are restored in plaster. Surface and breaks are very worn and rather abraded. The surface ist powdery to touch. The paint is worn."@en 
  ] ;
  crm:P108i_was_produced_by [ a crm:E12_Production ;
    rdfs:label "Production" ;
    crm:P4_has_time-span [ a crm:E52_Time-Span ;

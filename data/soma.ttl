    crm:P10_falls_within [ a crm:E4_Period ;
      crm:P1_is_identified_by [ a crm:E41_Appellation ;
        crm:P190_has_symbolic_content "Early Protogeometric to Middle Protogeometric"@en 
      ] ;
    ] ;
    crm:P32_used_general_technique somavoc:Wheelmade ; 
    crm:P32_used_general_technique somavoc:Three-phase_firing ;
    crm:P7_took_place_at [ a crm:E53_Place ;
      rdfs:label "Athens" ;
      crm:P168_place_is_defined_by "POINT(23.6559367 37.9909438)" 
    ] ;

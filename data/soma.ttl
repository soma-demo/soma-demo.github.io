  crm:P43_has_dimension [ a crm:E54_Dimension ;
    crm:P2_has_type somavoc:Depth_inside ;
    crm:P90_has_value 21 ; 
    crm:P91_has_unit somavoc:Centimeter 
  ] ;
  crm:P43_has_dimension [ a crm:E54_Dimension ;
    crm:P2_has_type somavoc:Width_slabs_of_schist ;
    crm:P90_has_value 10 ; 
    crm:P91_has_unit somavoc:Centimeter 
  ] ;
  crm:P43_has_dimension [ a crm:E54_Dimension ;
    crm:P2_has_type somavoc:Individuals ;

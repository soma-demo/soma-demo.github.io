#Type of enclosure (tomb)
somavoc:Cist_tomb a crm:E55_Type ;
    rdfs:label "Cist Tomb"@en ;
    skos:definition "This is a definition of Cist tomb" ;
    owl:sameAs aat:300005941 . 

#Unit of measurement
somavoc:Centimeter a crm:E58_Measurement_Unit ;
        rdfs:label "Centimeter"@en ;
        skos:definition "A unit of length in the metric system, equal to one hundredth (1/100) of a meter." ;
        owl:sameAs aat:300379098 .

#Measurement
somavoc:Length a crm:E55_Type ; 
  rdfs:label "Length"@en ;
  skos:definition "The measurement of something from end to end, or the longest dimension of an object." .

somavoc:Width a crm:E55_Type ; 
  rdfs:label "Width"@en ;
  skos:definition "The measurement of an object or space from side to side, perpendicular to its length." .

somavoc:Depth a crm:E55_Type ; 
  rdfs:label "Depth"@en  . 

somavoc:Length_outside a crm:E55_Type ; 
  rdfs:label "Length - outside"@en  .
